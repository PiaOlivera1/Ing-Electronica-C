# Project: propuesto_estudiantes
# Makefile created by Red Panda C++ 3.2.2958

CXX      = "g++.exe"
CC       = "gcc.exe"
WINDRES  = "windres.exe"
RM       = del /q /f
CD       = cd /d
LIBS     = 
INCS     = 
CXXINCS  = 
CXXFLAGS = $(CXXINCS) "-g3" "-pipe" "-Wall" "-D_DEBUG"
CFLAGS   = $(INCS) "-g3" "-pipe" "-Wall" "-D_DEBUG"
WINDRESFLAGS = 
RES      = propuesto_estudiantes_private.res
OBJ      = main.o $(RES)
BIN      = propuesto_estudiantes.exe
LINKOBJ  = "main.o" "propuesto_estudiantes_private.res"
CLEANOBJ = "main.o" "propuesto_estudiantes_private.res" "propuesto_estudiantes.exe"

.PHONY: all all-before all-after clean clean-custom

all: all-before $(BIN) all-after

clean: clean-custom
	-$(RM) $(CLEANOBJ) >NUL 2>&1

propuesto_estudiantes.exe: $(OBJ)

	$(CC) $(LINKOBJ) -o "propuesto_estudiantes.exe" $(LIBS)

main.o: main.c
	$(CC) -c "main.c" -o "main.o" $(CFLAGS)  -finput-charset=UTF-8 -fexec-charset=windows-1252

propuesto_estudiantes_private.res: propuesto_estudiantes_private.rc 
	$(WINDRES) -i "propuesto_estudiantes_private.rc" --input-format=rc -o "propuesto_estudiantes_private.res" -O coff $(WINDRESFLAGS) 

